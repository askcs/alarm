<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="nl.askcs.alarm" android:versionCode="1" android:versionName="1.0">

  <uses-sdk android:minSdkVersion="8"
            android:targetSdkVersion="16" />

  <uses-permission android:name="android.permission.INTERNET"></uses-permission>

  <application
      android:icon="@drawable/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/Theme.Alarm">

      <activity android:name=".ui.activity.MainActivity"
                android:label="@string/activity_label_main">
          <intent-filter>
              <action android:name="android.intent.action.MAIN"/>
              <category android:name="android.intent.category.LAUNCHER"/>
          </intent-filter>
      </activity>
      <activity android:label="@string/activity_label_alarm" android:name=".ui.activity.AlarmActivity" />
      <activity android:label="@string/activity_label_settings" android:name=".ui.activity.SettingsActivity" />

  </application>

</manifest>

